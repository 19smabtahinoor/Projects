{
    "manifest_version" : 2,
    "name" : "Covid-19 Live Update",
    "version" : "1.0",
    "description" : "Realtime Covid Update",
    "icons" : {
        "128" : "favicon.png",
        "48" : "favicon.png",
        "16" : "favicon.png"
    },
    "browser_action" : {
        "default_icon" : "favicon.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}